{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/fonts/(.*)",
      "dest": "/public/fonts/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/public/img/$1"
    },
    {
      "src": "/storage/(.*)",
      "dest": "/public/storage/$1"
    },
    {
      "src": "/video/(.*)",
      "dest": "/public/video/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/a-sarto-is-forever",
      "dest": "/public/en/a-sarto-is-forever.html"
    },
    {
      "src": "/faq-and-warranty",
      "dest": "/public/en/faq-and-warranty.html"
    },
    {
      "src": "/register-your-sarto",
      "dest": "/public/en/register-your-sarto.html"
    },
    {
      "src": "/contacts",
      "dest": "/public/en/contacts.html"
    },
    {
      "src": "/dealers",
      "dest": "/public/en/dealers.html"
    },
    {
      "src": "/antonio-enrico",
      "dest": "/public/en/antonio-enrico.html"
    },
    {
      "src": "/bikes",
      "dest": "/public/en/bikes.html"
    },
    {
      "src": "/collaborations",
      "dest": "/public/en/collaborations.html"
    },
    {
      "src": "/handmade-in-italy",
      "dest": "/public/en/handmade-in-italy.html"
    },
    {
      "src": "/journal",
      "dest": "/public/en/journal.html"
    },
    {
      "src": "/sarto-experience",
      "dest": "/public/en/sarto-experience.html"
    },
    {
      "src": "/sustainability",
      "dest": "/public/en/sustainability.html"
    },
    {
      "src": "/en/(.*)",
      "dest": "/public/en/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}

